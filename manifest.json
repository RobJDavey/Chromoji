{
  "name": "__MSG_extName__",
  "version": "0.1.1.0",
  "manifest_version": 2,
  "default_locale": "en_GB",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
	  "css": ["emoji.css"],
      "js": ["emoji.js"]
    }
  ],
  "web_accessible_resources": [
    "images/*.png"
  ]
}
