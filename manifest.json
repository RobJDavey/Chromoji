{
  "name": "__MSG_extName__",
  "description": "__MSG_desc__",
  "version": "1.2.2",
  "manifest_version": 2,
  "default_locale": "en_GB",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "homepage_url": "http://chromoji.blogspot.com/",
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
	  "css": ["emoji.css"],
      "js": ["jquery-1.9.1.min.js", "emoji.js"]
    }
  ],
  "web_accessible_resources": [
    "images/*.png",
    "chardict.json"
  ]
}
